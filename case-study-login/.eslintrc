{
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "requireConfigFile": false
    },
    "plugins": ["react","jsx","tsx"],
    "rules": {
      "react/prop-types": "off",
      "react/no-unknown-property": "off"
    }
  }
  